{
    "name": "DHT12",
    "Description": "Measure Temperature and Humidity",
    "PIN":{
        "Digital": 1,
        "Analog": 0,
        "I2C": false,
        "SPI": false,
        "Serial": false
    },
    "PINMODE":{
        "INPUT": 0,
        "OUTPUT": 1
    },
    "Library": ["DHT sensor library"],
    "header": [
            "#include <DHT.h>",
            "#define DHTPIN dhtpin_custom",
            "#define DHTTYPE DHT12",
            "DHT dht12(dht12pin_custom, DHTTYPE);"
            ],
    "custom key":["dht12pin_custom"],
    "code_in_setup": [
        "dht12.begin();"
    ],
    "code_function": [
        "float dht12_readTemperature(){t = dht12.readTemperature(); if(isnan(t)){return;}else{return t;}",
        "float dht12_readHumidity(){h = dht12.readHumidity(); if(isnan(h)){return;}else{return h;}"
    ],
    "function_list": ["dht12_readTemperature();", "dht12_readHumidity();"]

}